{
  "name": "@polywrap/wrappers.io",
  "version": "0.1.0",
  "main": "bin/main.js",
  "types": "bin/main.d.ts",
  "author": "nerfZael",
  "license": "MIT",
  "private": false,
  "files": [
    "/bin"
  ],
  "bin": {
    "ens-tracking-api": "./bin/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/polywrap/wrappers-io"
  },
  "keywords": [
    "polywrap",
    "wrap",
    "ens",
    "polywrapper",
    "wrapper"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json && yarn copy-ui-files",
    "dev": "ts-node src/main.ts",
    "start": "node bin/main.js",
    "test": "yarn test:jest",
    "copy-ui-files": "npx rimraf ./bin/ui && mkdir -p ./bin/ui/ && npx copyfiles -u 2 ./src/ui/* ./bin/ui/",
    "test:jest": "jest test --passWithNoTests"
  },
  "dependencies": {
    "awilix": "^6.0.0",
    "axios": "^0.24.0",
    "commander": "^8.3.0",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "ethers": "^5.5.2",
    "express": "^4.17.2",
    "pg": "^8.7.3",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.1",
    "@types/node": "^16.11.11",
    "@types/pg": "^8.6.5",
    "@web3api/cli": "0.0.1-prealpha.64",
    "@web3api/test-env-js": "0.0.1-prealpha.64",
    "copyfiles": "^2.4.1",
